    "development"
  ],
  "hints": {
    "axe/text-alternatives": "off",
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "axe/structure": "off",
    "axe/name-role-value": "off",
    "disown-opener": "off",
    "no-inline-styles": "off",
    "axe/language": "off",
    "meta-charset-utf-8": "off",
    "meta-viewport": "off",
    "button-type": "off",
    "axe/keyboard": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "@property",
          "@property",
          "@property"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 139",
    "not chrome <= 139",
    "not edge <= 139",
    "not firefox <= 142",
    "not safari <= 18.6",
    "not samsung <= 28"
  ]
